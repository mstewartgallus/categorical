(* Extend the language with some basic integer extras. *)
check zset _ set.
check (z _) _ zset.
check add (exists zset (fun x => exists zset (fun y => _))) zset.

apply (compose add (lift (z A))) (lift (z B)) (z C) :-
    plus A B C.
apply add (compose (lift (z A)) (lift (z B))) (z C) :-
    plus A B C.
